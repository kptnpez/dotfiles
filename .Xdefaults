! FONTS
!urxvt*font: xft:AnonymousPro:size=8:antialias=true
!urxvt*boldFont: xft:AnonymousPro:size=8:antialias=true

URxvt*font: xft:berry:style=Regular:size=9:antialias=false
URxvt*boldFont: xft:berry:style=Regular:size=9:antialias=false

!URxvt*font: xft:Profont:style=Regular:size=9:antialias=false
!URxvt*boldFont: xft:Profont:style=Regular:size=9:antialias=false
!URxvt*italicFont: xft:Profont:style=Regular:size=11:antialias=false URxvt*bolditalicfont: xft:Profont:style=Regular:size=11:antialias=false

!urxvt*font: xft:Gohufont:size=11:antialias=true
!urxvt*boldFont: xft:Gohufont:size=11:antialias=true
!urxvt*font: xft:SourceCodePro:size=10:antialias=true
!urxvt*boldFont: xft:SourceCodePro:size=10:antialias=true

!urxvt*font: xft:Hermit:size=9:antialias=true
!urxvt*boldFont: xft:Hermit:size=9:antialias=true

!urxvt*font: xft:cure:size=8:antialias=true
!urxvt*boldFont: xft:cure:size=8:antialias=true


! TABS COLOURS
! URxvt.color0  : #979797
! URxvt.color1  : #000000

! SETTINGS
URxvt*urgentOnBell: True
URxvt*scrollBar: False
! URxvt*scrollColor: #8FF528
! URxvt*troughColor: #222222
URxvt*secondaryScroll: True
URxvt*saveLines: 1000
URxvt*geometry: 78x24
URxvt.cursorUnderline: False
URxvt.cursorBlink: True
URxvt*underlineColor: #181818
! URxvt*depth: 32
! URxvt*transparent: True
! URxvt*borderColor: #C2FFA0

! CLICKABLE LINKS
URxvt.perl-ext-common:      default,matcher
URxvt.matcher.button:       1
URxvt.urlLauncher:          x-www-browser

!URL Support 
URxvt.perl-ext:			default,url-select
URxvt.keysym.M-u:		perl:url-select:select_next
!URxvt.url-select.launcher:	/usr/bin/chromium -new-tab
!URxvt.url-select.launcher:	/usr/bin/firefox -new-tab
URxvt.url-select.launcher:	/usr/bin/firedev -new-tab
URxvt.url-select.underline:	true

! TABS SETTINGS
! URxvt.tabbed.tabbar-fg: 1
! URxvt.tabbed.tabbar-bg: 0
! URxvt.tabbed.tab-fg:    1
! URxvt.tabbed.tab-bg:    0
